package lista6;

import java.util.Scanner;

public class L6Q1 {

public void main(String [] args) {
	    Scanner in = new Scanner(System.in);
	    Lista lista = new Lista();
	    int numero=0;
	    boolean fimLista=false;
	    int numSorteado=0;
	    while(in.hasNextInt()) {
	    	numero = in.nextInt();
	    if(fimLista) {
	    	numSorteado=numero;
	    	}else if(numero!=-1 && !fimLista) {
	    		lista.inserir(numero);
	    	}else if(numero==-1) {
	    		fimLista=true;
	    	}
	    }
	    in.close();
		}
}



class Lista{
    private int number;
    private Lista proximo;
    public Lista(){
        this.number=0;
        this.proximo=null;
    }
    public void inserir(int numero){
        if (this.number==0){
            this.number=numero;
            this.proximo=new Lista();
        }else{
            this.proximo.inserir(numero);
        }
    }
    public void remover(int numero){
        if(this.number!=0){
            if(this.number==numero){
                this.number=this.proximo.number;
                this.proximo=this.proximo.proximo;
            }else{
                this.proximo.remover(numero);
            }
        }else{
            throw new RuntimeException("Erro");
        }
    }
    public boolean tem(int numero){
        if(this.number==numero){
            return true;
        }else if(this.proximo != null){
            return this.proximo.tem(numero);
        }else{
            return false;
        }
    }
}
